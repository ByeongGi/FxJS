{
  "name": "fxjs2",
  "version": "0.2.7",
  "description": "Functional Extensions for Javascript",
  "main": "index.js",
  "module": "fx.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build-modern && npm run build-legacy",
    "build-modern": "webpack --config modern.config.js --env.NODE_ENV=production",
    "build-legacy": "webpack --config legacy.config.js --env.NODE_ENV=production",
    "test": "mocha -R spec test/**/*.js"
  },
  "files": [
    "Concurrency/*",
    "Lazy/*",
    "*.js",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marpple/FxJS.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marpple/FxJS/issues"
  },
  "homepage": "https://github.com/marpple/FxJS#readme",
  "dependencies": {
    "core-js": "^3.0.1",
    "esm": "^3.0.80",
    "regenerator-runtime": "^0.13.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
